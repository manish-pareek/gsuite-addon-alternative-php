{
    "name": "manish-pareek/gsuite-addon-php",
    "description": "For the gsuite addon card services alternatives",
    "keywords": [
        "manish-pareek",
        "gsuite-addon-alternative-php"
    ],
    "homepage": "https://github.com/manish-pareek/gsuite-addon-alternative-php",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Manish Pareek",
            "email": "pareek.manish558@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6|^7|^8|^9"
    },
    "autoload": {
        "psr-4": {
            "ManishPareek\\Gsuite\\Addon\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ManishPareek\\Gsuite\\Addon\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}
